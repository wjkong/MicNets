using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace MicNets.Model
{
    public class LinkInfo
    {
        // Fields
        private NTaskLink _currentTask;
        private bool _isExternal;
        private NLinkCategory _linkCategory;
        private int _linkID;
        private int _parentLinkID;
        private short _roleID;
        private int _sequence;
        private string _sessionID;
        private string _text;
        private string _url;
        private string _culture;

        public string Culture
        {
            get { return _culture; }
            set { _culture = value; }
        }

        // Properties
        public NTaskLink CurrentTask
        {
            get
            {
                return this._currentTask;
            }
            set
            {
                this._currentTask = value;
            }
        }

        public bool IsExternal
        {
            get
            {
                return this._isExternal;
            }
            set
            {
                this._isExternal = value;
            }
        }

        public NLinkCategory LinkCategory
        {
            get
            {
                return this._linkCategory;
            }
            set
            {
                this._linkCategory = value;
            }
        }

        public int LinkID
        {
            get
            {
                return this._linkID;
            }
            set
            {
                this._linkID = value;
            }
        }

        public int ParentLinkID
        {
            get
            {
                return this._parentLinkID;
            }
            set
            {
                this._parentLinkID = value;
            }
        }

        public short RoleID
        {
            get
            {
                return this._roleID;
            }
            set
            {
                this._roleID = value;
            }
        }

        public int Sequence
        {
            get
            {
                return this._sequence;
            }
            set
            {
                this._sequence = value;
            }
        }

        public string SessionID
        {
            get
            {
                return this._sessionID;
            }
            set
            {
                this._sessionID = value;
            }
        }

        public string Text
        {
            get
            {
                return this._text;
            }
            set
            {
                this._text = value;
            }
        }

        public string URL
        {
            get
            {
                return this._url;
            }
            set
            {
                this._url = value;
            }
        }
    }
}
